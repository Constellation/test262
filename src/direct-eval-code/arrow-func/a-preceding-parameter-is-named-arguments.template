assert.sameValue("arguments" in this, false, "No global 'arguments' binding");

const f = (arguments, p = /*{ parameter-code }*/) => {}
assert.throws(SyntaxError, f);


assert.sameValue("arguments" in this, false, "No global 'arguments' binding");
